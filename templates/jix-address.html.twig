<div class="panel panel-primary">
    <div class="panel-heading"><i class="fas fa-home"></i> {{ title }}</div>
    <div class="panel-body">
        <p>{{ drupal_token('site:name') }} - {{ owner }}</p>
        <p><i class="fas fa-map-marker-alt"></i> {{ address1 }} </p>
        <p>{{ address2 }}</p>
        <p><i class="fas fa-phone"></i> {{ phone }}</p>
        <p><i class="fas fa-at"></i> {{ drupal_token('site:mail') }}</p>
    </div>
</div>